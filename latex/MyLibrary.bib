@article{cohnDifferentialEffectsLipopolysaccharide2006,
  title = {Differential Effects of Lipopolysaccharide in the Social Behavior of Dominant and Submissive Mice},
  author = {Cohn, Daniel Wagner Hamada and family=Sá-Rocha, given=Luiz Carlos, prefix=de, useprefix=true},
  date = {2006-05-30},
  journaltitle = {Physiology \& Behavior},
  shortjournal = {Physiology \& Behavior},
  volume = {87},
