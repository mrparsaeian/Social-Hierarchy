@article{hanwrightStimulatedGripStrength2019,
  title = {Stimulated Grip Strength Measurement: {{Validation}} of a Novel Method for Functional Assessment},
  shorttitle = {Stimulated Grip Strength Measurement},
  author = {Hanwright, Philip J. and Rath, Jennifer L. and family=Guionneau, given=Nicholas, prefix=von, useprefix=true and Harris, Thomas G.W and Sarhane, Karim A. and Kemp, Stephen W.P. and Hoke, Ahmet and Cederna, Paul S. and Tuffaha, Sami H.},
