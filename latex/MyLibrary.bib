  type = {preprint},
  title = {{{AI-driven}} High-Throughput Automation of Behavioral Analysis and Dual-Channel Wireless Optogenetics for Multiplexing Brain Dynamics},
  author = {Kim, Woo Seok and Liu, Jianfeng and Li, Qinbo and Hong, Sungcheol and Qi, Kezhuo and Cherukuri, Rahul and Yoon, Byung-Jun and Moscarello, Justin and Choe, Yoonsuck and Maren, Stephen and Park, Sung Il},
