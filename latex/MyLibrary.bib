@article{netserDistinctDynamicsSocial2020,
  title = {Distinct Dynamics of Social Motivation Drive Differential Social Behavior in Laboratory Rat and Mouse Strains},
  author = {Netser, Shai and Meyer, Ana and Magalnik, Hen and Zylbertal, Asaph and family=Zerda, given=Shani Haskal, prefix=de la, useprefix=true and Briller, Mayan and Bizer, Alexander and Grinevich, Valery and Wagner, Shlomo},
  date = {2020-11-20},
  journaltitle = {Nature Communications},
  shortjournal = {Nat Commun},
  volume = {11},
  number = {1},
  pages = {5908},
  publisher = {Nature Publishing Group},
  issn = {2041-1723},
  doi = {10.1038/s41467-020-19569-0},
  url = {https://www.nature.com/articles/s41467-020-19569-0},
  urldate = {2024-06-17},
  abstract = {Mice and rats are widely used to explore mechanisms of mammalian social behavior in health and disease, raising the question whether they actually differ in their social behavior. Here we address this question by directly comparing social investigation behavior between two mouse and rat strains used most frequently for behavioral studies and as models of neuropathological conditions: C57BL/6\,J mice and Sprague Dawley (SD) rats. Employing novel experimental systems for behavioral analysis of both subjects and stimuli during the social preference test, we reveal marked differences in behavioral dynamics between the strains, suggesting stronger and faster induction of social motivation in SD rats. These different behavioral patterns, which correlate with distinctive c-Fos expression in social motivation-related brain areas, are modified by competition with non-social rewarding stimuli, in a strain-specific manner. Thus, these two strains differ in their social behavior, which should be taken into consideration when selecting an appropriate model organism.},
  langid = {english},
  keywords = {Limbic system,Motivation,Reward,Social behaviour},
  file = {/home/master/PJ/Zotero/storage/CIF4XKNF/10.1038@s41467-020-19569-0.pdf.pdf;/home/master/PJ/Zotero/storage/XIN2ITML/Netser et al. - 2020 - Distinct dynamics of social motivation drive diffe.pdf}
}

@article{oliveiraWhyWinnersKeep2009,
  title = {Why Do Winners Keep Winning? {{Androgen}} Mediation of Winner but Not Loser Effects in Cichlid Fish},
  shorttitle = {Why Do Winners Keep Winning?},
  author = {Oliveira, Rui F. and Silva, Ana and Canário, Adelino V.M.},
  date = {2009-03-11},
  journaltitle = {Proceedings of the Royal Society B: Biological Sciences},
  volume = {276},
  number = {1665},
  pages = {2249--2256},
  publisher = {Royal Society},
  doi = {10.1098/rspb.2009.0132},
  url = {https://royalsocietypublishing.org/doi/10.1098/rspb.2009.0132},
  urldate = {2024-06-21},
  abstract = {Animal conflicts are influenced by social experience such that a previous winning experience increases the probability of winning the next agonistic interaction, whereas a previous losing experience has the opposite effect. Since androgens respond to social interactions, increasing in winners and decreasing in losers, we hypothesized that socially induced transient changes in androgen levels could be a causal mediator of winner/loser effects. To test this hypothesis, we staged fights between dyads of size-matched males of the Mozambique tilapia (Oreochromis mossambicus). After the first contest, winners were treated with the anti-androgen cyproterone acetate and losers were supplemented with 11-ketotestosterone. Two hours after the end of the first fight, two contests were staged simultaneously between the winner of the first fight and a naive male and between the loser of first fight and another naive male. The majority (88\%) of control winners also won the second interaction, whereas the majority of control losers (87\%) lost their second fight, thus confirming the presence of winner/loser effects in this species. As predicted, the success of anti-androgen-treated winners in the second fight decreased significantly to chance levels (44\%), but the success of androgenized losers (19\%) did not show a significant increase. In summary, the treatment with anti-androgen blocks the winner effect, whereas androgen administration fails to reverse the loser effect, suggesting an involvement of androgens on the winner but not on the loser effect.},
  keywords = {aggression,androgens,social experience,testosterone,winner effect},
  file = {/home/master/PJ/Zotero/storage/SP24BPQP/oliveira2009.pdf.pdf;/home/master/PJ/Zotero/storage/TTITGJ7A/Oliveira et al. - 2009 - Why do winners keep winning Androgen mediation of.pdf}
}

@article{olssonNeuralComputationalSystems2020,
  title = {The Neural and Computational Systems of Social Learning},
  author = {Olsson, Andreas and Knapska, Ewelina and Lindström, Björn},
  date = {2020-04},
  journaltitle = {Nature Reviews Neuroscience},
  shortjournal = {Nat Rev Neurosci},
  volume = {21},
  number = {4},
  pages = {197--212},
  publisher = {Nature Publishing Group},
  issn = {1471-0048},
  doi = {10.1038/s41583-020-0276-4},
  url = {https://www.nature.com/articles/s41583-020-0276-4},
  urldate = {2024-06-21},
  abstract = {Learning the value of stimuli and actions from others — social learning — adaptively contributes to individual survival and plays a key role in cultural evolution. We review research across species targeting the neural and computational systems of social learning in both the aversive and appetitive domains. Social learning generally follows the same principles as self-experienced value-based learning, including computations of prediction errors and is implemented in brain circuits activated across task domains together with regions processing social information. We integrate neural and computational perspectives of social learning with an understanding of behaviour of varying complexity, from basic threat avoidance to complex social learning strategies and cultural phenomena.},
  langid = {english},
  keywords = {Neural circuits,Sensory processing},
  file = {/home/master/PJ/Zotero/storage/E7EPKBYZ/Olsson et al. - 2020 - The neural and computational systems of social lea.pdf}
}

