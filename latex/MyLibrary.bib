@article{chavira-suarezDevelopmentalProgrammingAgeing2023,
  title = {Developmental Programming–Ageing Effects in Muscle Strength of Obese Rat Offspring in a Sex-Dependent Manner},
  author = {Chavira-Suárez, Erika and Ibañez, Carlos Alberto and Castro-Rodríguez, Diana Catalina and Reyes-Castro, Luis Antonio and Ramírez-Leal, Marina Gisela and Martínez-Mota, Lucía Alba and Zambrano, Elena},
